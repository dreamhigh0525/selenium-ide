{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "stop chromedriver",
            "command": "taskkill",
            "type": "process",
            "args": [
                "/im",
                "chromedriver.exe",
                "/f",
                "/FI",
                "IMAGENAME eq chromedriver.exe"
            ],
            "presentation": {
                "reveal": "always"
            }
        }
    ]
}