{
  "env": {
    "test": {
      "presets": ["node6"]
    },
    "development": {
      "presets": [
      {
        "env": {
          "modules": false
        }
      }],
      "plugins": [
        "external-helpers",
        "transform-decorators-legacy",
        "transform-decorators",
        "transform-object-rest-spread",
        ["babel-plugin-transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }]
      ]
    }
  }
}
