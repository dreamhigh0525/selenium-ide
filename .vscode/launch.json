{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main Process",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "sourceMaps": true,
            "trace": true,
            "windows": {
                "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron"
            },
            "args": [
                ".\\packages\\selenium-ide\\dist\\main-bundle.js"
            ],
            "outputCapture": "std"
        }
    ]
}
